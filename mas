# leap-year-
#include<stdio.h>
int main()
{
int year;
printf("enter the year you want to check \n");
scanf("%d",&year);

if(year%4==0)
   {
     if(year%100==0)
       {
         if(year%400==0)
          {
            printf(" %d the year is a leap year \n",year);
           }    
           else 
             printf("%d is not a leap year \n",year);
           
           }
           }
         else 
         printf("%d is not a leap year \n",year);
    return (0);
}
