# leap-year-
#include<stdio.h>
int main()
{
int year;
printf("enter the year you want to check \n");
scanf("%d",&year);

if((year%4==0)&&(year%100==0)||(year%100!=0))
          { printf(%d year is leap year)
          }
           else {
             printf("%d is not a leap year \n",year)
             }
    return (0);
}
